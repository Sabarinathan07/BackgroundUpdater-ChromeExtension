{

    "manifest_version": 2,
    "name": "backgroundUpdater",
    "description": "Adding a icon in websites",
    "version": "1.0",
    "icons": {
        "48":"/icon.png"
    },

    "page_action":{
        "default_icon":{
            "48":"/icon.png"
        }
    }, 
    
    "content_scripts":[{
        "matches":["https://www.google.co.in/*"],
        "css":["styles.css"]
    }]
   
   
}
  